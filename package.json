    "ignore": [
      "expo",
      "react",
      "react-dom",
      "react-native",
      "react-test-renderer",
      "react-transition-group"
    ]
  },
  "spin": {
    "presets": {
      "apollo-react-server": true,
      "apollo-react-web": true,
      "apollo-react-android": false,
      "apollo-react-ios": false,
      "apollo-react-server-test": true
    },
    "options": {
      "backendUrl": "http://{ip}:8080/graphql",
      "ssr": true,
      "webpackDll": true,
      "reactHotLoader": false,
      "persistGraphQL": false,
      "frontendRefreshOnBackendChange": true,
      "reactHotLoader": false,
      "persistGraphQL": false
    }
  }
}
